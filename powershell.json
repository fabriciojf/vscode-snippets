{
    "ps-case": {
        "prefix": [
            "ps1-case",
            "shell-case",
            "ps-case"
        ],
        "body": [
            "switch (\\$args[0]) {",
            "    \"term1\" {",
            "        fprint \"description\"",
            "    }",
            "    {(\\$_ -eq \"term2\") -or (\\$_ -eq \"term3\")} {",
            "        Set-Location \"C:\\Users\\fabri\\Documents\\Workspace\\backups\"",
            "    }",
            "    default {",
            "    }",
            "}"
        ],
        "description": "Get-ChildItem"
    },
    "ps-usage": {
        "prefix": [
            "ps1-usage",
            "shell-usage",
            "ps-usage"
        ],
        "body": [
            "default {",
            "    fprint \"\"",
            "    fprint \"Usage: command [Action]\"",
            "    fprint \"\"",
            "    fprint \"Actions:\"",
            "    fprint \"\"",
            "    fdestak \"   action  \" \"description\"",
            "}"
        ],
        "description": "Get-ChildItem"
    },
    "ps-command": {
        "prefix": [
            "ps1-command-custom",
            "shell-command-custom",
            "ps-command-custom"
        ],
        "body": [
            "\\$arg1 = $args[0]",
            "switch (\\$arg1) {",
            "    \"term1\" {",
            "        Write-Host \\$arg1 -ForegroundColor yellow",
            "    }",
            "    {(\\$_ -eq \"term2\") -or (\\$_ -eq \"term3\")} {",
            "        Write-Host \\$arg1 -ForegroundColor red",
            "    }",
            "    default {",
            "        fprint \"\"",
            "        fprint \"Usage: command [Action]\"        ",
            "        fprint \"\"",
            "        fprint \"Actions\"",
            "        fprint \"\"",
            "        fdestak \"   action [ param ]  \" \"description\"",
            "        fdestak \"   action [ param ]  \" \"description\"",
            "        fprint \"\"",
            "    }",
            "}"
        ],
        "description": "Custom Command"
    },
    "VerificarNulo": {
        "prefix": "verificarnulo",
        "body": [
            "function VerificarNulo($parametro) {",
            "\tif ($parametro -eq $null) {",
            "\t\tWrite-Host \"O parâmetro é nulo.\"",
            "\t} else {",
            "\t\tWrite-Host \"O parâmetro não é nulo.\"",
            "\t}",
            "}"
        ],
        "description": "Checks if a parameter is null"
    }
}
