{
    "bash-if": {
        "prefix": "bash-if",
        "body": [
            "# contains",
            "if [[ \"\\$2\" != *\".log\"* ]]; then",
            "    echo \"True\"",
            "# é igual ao texto or [ a -eq b]",
            "elif [[ \"\\$2\" == \".txt\" ]]",
            "    echo \"False\"",
            "# é igual ao numero",
            "elif [[ \"\\$2\" -eq 0 ]]",
            "    echo \"False\"",
            "# é maior que o numero or [ a -gt b]",
            "elif [[ \"\\$2\" > 10 ]]",
            "    echo \"False\"",
            "# é menor que o numero or [ a < b]",
            "elif [[ \"\\$2\" -lt 10 ]]",
            "    echo \"False\"",
            "else",
            "    echo \"False\"",
            "fi"
        ],
        "description": "Bash IF"
    },
    "bash-case": {
        "prefix": "bash-case",
        "body": [
            "case \\$1 in",
            "    \"\")",
            "        echo \"\\empty\"",
            "    ;;",
            "    \"v1\")",
            "        echo \"\\v1\"",
            "    ;;",
            "    *)",
            "        echo \"Informe o parametro\"",
            "    ;;",
            "esac"
        ],
        "description": "Bash Case"
    },
    "bash-function": {
        "prefix": "bash-function",
        "body": [
            "function print() {",
            "    echo \"message: \\$1\"",
            "}",
            "# calling",
            "print \"teste\""
        ],
        "description": "Bash Function"
    },
    "bash-command-n-lines": {
        "prefix": "bash-command-n-lines",
        "body": [
