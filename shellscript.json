{
    "bash-template": {
        "prefix": "bash-template",
        "body": [
            "",
            "case \\$1 in",
            "    'start')",
            "        destak 'Command' 'Description'",
            "        print ''",
            "        # command",
            "    ;;",
            "    'build') ;& 'run')",
            "        destak 'Command' 'Description'",
            "        print ''",
            "        # command",
            "    ;;",
            "    *)",
            "        print ''",
            "        print 'Usage: command [action] [options]' white",
            "        print ''",
            "        print 'Actions:' white",
            "        print ''",
            "        destak '   action      ' 'Description'",
            "        print ''",
            "    ;;",
            "esac",
        ],
        "description": "Bash Template"
    },
    "bash-if": {
        "prefix": "bash-if",
        "body": [
            "#--------------- start IF\n",
            "# Verifica se o argumento é nulo",
            "if [ -z \"\\$2\" ]; then",
            "\techo \"O argumento é nulo.\"",
            "\n# contains",
            "elif [[ \"\\$2\" != *\".log\"* ]]; then",
            "\techo \"True\"",
            "\n# é igual ao texto or [ a -eq b]",
            "elif [[ \"\\$2\" == \".txt\" ]]; then",
            "\techo \"False\"",
            "elif [[ \\$station == \"pc\" ]]; then",
            "\techo 'pc'",
            "elif [[ \"\\$station\" == \"note\" ]]; then",
            "\techo 'Notebook'",
            "\n# é igual ao numero",
            "elif [[ \"\\$2\" -eq 0 ]]; then",
            "\techo \"False\"",
            "\n# é maior que o numero or [ a -gt b]",
            "elif [[ \"\\$2\" > 10 ]]; then",
            "\techo \"False\"",
            "\n# é menor que o numero or [ a < b]",
            "elif [[ \"\\$2\" -lt 10 ]]; then",
            "\techo \"False\"",
            "\n# senao",
            "else",
            "\techo \"False\"",
            "fi",
            "\n#--------------- finish IF"
        ],
        "description": "Bash IF"
    },
    "bash-case": {
        "prefix": "bash-case",
        "body": [
            "case \\$1 in",
            "    '')",
            "        echo '\\empty'",
            "    ;;",
            "    'v1') ;& 'v2'",
            "        echo 'v1-v2'",
            "    ;;",
            "    *)",
            "        echo 'Informe o parametro'",
            "    ;;",
            "esac"
        ],
        "description": "Bash Case"
    },
    "bash-function": {
        "prefix": "bash-function",
        "body": [
            "function print() {",
            "    echo 'message: \\$1'",
            "}",
            "# calling",
            "print 'teste'"
        ],
        "description": "Bash Function"
    },
    "bash-command-n-lines": {
        "prefix": "bash-command-n-lines",
        "body": [
            "node prompt.js service=OMNIDEMO \\",
            "    user=dnk \\",
            "    url=http://localhost:3130/chassi \\",
            "    media=message"
        ],
      "description": "Bash Function"
    }
}
